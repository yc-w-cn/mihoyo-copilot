{
  "name": "@mihoyo-copilot/biligame",
  "private": true,
  "version": "1.0.0",
  "description": "Simple cli application to download meta info from biligame.",
  "author": "Yuchen Wang <contact@wangyuchen.cn>",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "biligame": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "ys:dev": "ts-node ./src/cli.ts --game ys",
    "sr:dev": "ts-node ./src/cli.ts --game sr",
    "bh3:dev": "ts-node ./src/cli.ts --game bh3",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "pnpm run clean && tsc",
    "test": "pnpm run build && ava --verbose"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "ava": "^6.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "commander": "^12.0.0"
  }
}
